"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/v_crowbar/v_crowbar_green"
	"$surfaceprop"	"default"
	"$bumpmap"	"models/weapons/v_crowbar/v_crowbar_normal"
	"$envmap"	"env_cubemap"
	"$envmaptint" "[.5 .5 .5]"

	//phong properties
	"$phong"	"1"
	"$phongboost"	"2.5"
	"$phongexponent"	"50"
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$rimlight" "1"
	"$rimlightexponent" "5"	
	"$rimlightboost" "3"

	"$ambientocclusion" "1"

	"360?$color2" "[ 0.9 0.8 0.8 ]"
	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"vm_invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}

}